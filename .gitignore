# Ignore bundler config.
/.bundle
/vendor/bundle/

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
/.cache

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

/public/assets
.byebug_history
pry_history

# Ignore master key for decrypting credentials and more.
/config/master.key
.env

/public/packs
/public/packs-test
/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity
.yarnrc
